{
  "name": "my-app",
  "productName": "my-app",
  "version": "1.0.4",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "zhangzhuang",
    "email": "zhangzhuang@163.com"
  },
  "build": {
    "publish": {
      "provider": "generic",
      "url": "http://localhost/electron-updates"
    },
    "generateUpdatesFilesForAllChannels": true,
    "updaterCacheDirName": "my-app-updater",
    "win": {
      "extraResources": [
        {
          "from": "node_modules/electron/dist/locales",
          "to": "locales",
          "filter": ["zh-CN.pak", "en-US.pak"]
        }
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron-forge/plugin-vite": "^7.8.3",
    "@electron-forge/publisher-github": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "electron": "^38.0.0",
    "vite": "^5.4.19"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "update-electron-app": "^3.1.1"
  }
}
